@article{an2016reports,
 abstract = {The Workshop Program of the Association for the Advancement of Artificial Intelligence’s International Conference on Web and Social Media (AAAI-16) was held in Cologne, Germany. There were 6 workshops in the program: CityLab(W1), Ethical Social Media Research (W2), Social Media and Demographic Research (W3), Wiki (W4), #Fail: Thinks that Didn’t Work Out (W5), News and Public Opinion (W6), Social Media in the Newsroom (W7), and Social Web for Environmental and Ecological Minoitoring (W8). Workshops were held on the first day of the conference, Tuesday, May 17, 2016. Workshop participants met and discussed issues with a selected focus — providing an informal setting for active exchange among researchers, developers and users on topics of current interest. There were 8 workshops held at the conference, organizers from 5 chose to include papers in the AAAI Technical Reports series and organizers from 6 workshops submitted reports.},
 author = {An, Jisun and Crandall, David J. and Fedorov, Roman and Fiesler, Casey and Giglietto, Fabio and Heravi, Bahareh and Pater, Jessica and Pelechrinis, Konstantinos and Quercia, Daniele and Weller, Katrin and Zubiaga, Arkaitz},
 doi = {10.1609/aimag.v37i4.2692},
 journal = {AI Magazine},
 month = {Jan.},
 number = {4},
 pages = {89-93},
 title = {Reports of the Workshops Held at the 2016 International AAAI Conference on Web and Social Media},
 url = {https://www.aaai.org/ojs/index.php/aimagazine/article/view/2692},
 volume = {37},
 year = {2017}
}

